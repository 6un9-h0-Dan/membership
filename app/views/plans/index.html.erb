<p id="notice"><%= notice %></p>

<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.name %></td>
        <td><%= plan.description %></td>
        <td>$<%= plan.amount %></td>
        <td><%= link_to 'Show', plan %></td>
      </tr>
    <% end %>
  </tbody>
</table>

